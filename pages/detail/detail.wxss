.dashboard {
  width: 100%;
  padding: 1rem;
  box-sizing: border-box;
  box-shadow: 0 -0.1rem 0.2rem rgba(96, 49, 152, 0.5);
}
.dashboard .buttons {
  width: 100%;
}
.dashboard .buttons button {
  background-color: #7a6ed5;
  width: 220rpx;
  font-size: 18px;
  border-radius: 40rpx;
  border: none;
  color: #fff;
  height: 90rpx;
  line-height: 90rpx;
  vertical-align: middle;
  margin: 30rpx auto;
  box-shadow: 0 0 5px #7a6ed5;
}
.dashboard .buttons button:first-child {
  margin-top: 0;
}
.dashboard .buttons button:last-child {
  border-right-width: 1px;
}
.dashboard .buttons button:focus {
  outline: 0;
}
