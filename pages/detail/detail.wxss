.game-wrap {
  height: 100%;
  background-color: #f0f4f8;
}
.hidden {
  display: none;
}
.container {
  background-color: #3a1e78;
  height: 650rpx;
}
.container .row {
  width: 100%;
  border-color: #ccc;
  height: 82rpsx;
  display: flex;
}
.container .row > span {
  width: 11%;
  display: inline-block;
  cursor: pointer;
  text-align: center;
  height: 70rpx;
  line-height: 70rpx;
  color: #fff;
  border: 1px solid #ccc;
  background-color: #3a1e78;
  border-right-width: 0;
  border-bottom-width: 0;
}
.container .row > span.light {
  background-color: #603198;
}
.container .row > span.col_g_right {
  border-right-width: 1px;
}
.container .row > span:last-child {
  border-right-width: 2px;
}
.container .row > span:first-child {
  border-left-width: 2px;
}
.container .row > span.fixed {
  background-color: #eee;
}
.container .row > span.empty {
  color: transparent;
}
.container .row > span.error {
  background-color: black;
}
.container .row > span.error.empty {
  color: black;
}
.container .row > span.mark1 {
  background-color: teal;
}
.container .row > span.mark1.empty {
  color: teal;
}
.container .row > span.mark3 {
  background-color: pink;
}
.container .row > span.mark3.empty {
  color: pink;
}
.container .row.row_g_bottom > span {
  border-bottom-width: 1px;
}
.container .row:first-child > span {
  border-top-width: 2px;
}
.container .row:last-child > span {
  border-bottom-width: 2px;
}
